{
	"name": "nest-typescript-starter",
	"version": "1.0.0",
	"description": "Nest TypeScript starter repository",
	"license": "MIT",
	"scripts": {
		"format": "prettier --write \"**/*.ts\"",
		"start": "ts-node -r tsconfig-paths/register src/main.ts",
		"start:dev": "nodemon",
		"prestart:prod": "rm -rf dist && tsc",
		"start:prod": "node dist/main.js",
		"test": "jest",
		"test:cov": "jest --coverage",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"run:analysis": "ts-node src/script-runner/analysis.ts",
		"run:suspension": "ts-node src/script-runner/suspension/suspension.ts",
		"run:quote": "ts-node src/script-runner/test/quote.test.ts",
		"run:daily": "ts-node src/script-runner/daily-run/daily.run.ts"
	},
	"dependencies": {
		"@nestjs/common": "^5.0.1",
		"@nestjs/core": "^5.0.1",
		"@nestjs/microservices": "^5.0.1",
		"@nestjs/swagger": "^1.2.2",
		"@nestjs/testing": "^4.5.5",
		"@nestjs/websockets": "^5.0.1",
		"@types/node-schedule": "^1.2.2",
		"@types/node-telegram-bot-api": "^0.30.1",
		"axios": "^0.18.0",
		"emojis": "^1.0.10",
		"firebase-admin": "^5.12.1",
		"html-to-image": "0.0.2",
		"mssql": "^4.1.0",
		"node-emoji": "^1.8.1",
		"node-mssql": "0.0.1",
		"node-schedule": "^1.3.0",
		"node-telegram-bot-api": "^0.30.0",
		"reflect-metadata": "^0.1.12",
		"regression": "^2.0.1",
		"request": "^2.85.0",
		"request-promise": "^4.2.2",
		"rxjs": "^6.2.1",
		"rxjs-compat": "^6.2.1",
		"socket.io": "^2.1.1",
		"technicalindicators": "^1.1.11",
		"typeorm": "^0.2.4",
		"typescript": "^2.6.2",
		"url-to-image": "^1.0.0",
		"webworker-threads": "^0.7.16",
		"xml2json": "^0.11.2",
		"yahoo-finance": "^0.3.5"
	},
	"devDependencies": {
		"@types/express": "^4.0.39",
		"@types/jest": "^21.1.8",
		"@types/node": "^9.3.0",
		"@types/supertest": "^2.0.4",
		"jest": "^21.2.1",
		"nodemon": "^1.14.1",
		"prettier": "^1.11.1",
		"supertest": "^3.0.0",
		"ts-jest": "^21.2.4",
		"ts-node": "^4.1.0",
		"tsconfig-paths": "^3.1.1",
		"tslint": "^5.3.2"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"coverageDirectory": "../coverage"
	}
}
